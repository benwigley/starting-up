<!DOCTYPE html>
<html>
  <head>
    <title>StartingUp</title>
    <%= csrf_meta_tags %>


    <link rel="apple-touch-icon" sizes="180x180" href="<%= image_path('favicons/apple-touch-icon.png') %>">
    <link rel="icon" type="image/png" href="<%= image_path('favicons/favicon-32x32.png') %>" sizes="32x32">
    <link rel="icon" type="image/png" href="<%= image_path('favicons/favicon-16x16.png') %>" sizes="16x16">
    <link rel="manifest" href="<%= image_path('favicons/manifest.json') %>">
    <link rel="mask-icon" href="<%= image_path('favicons/safari-pinned-tab.svg') %>" color="#777777">
    <link rel="shortcut icon" href="<%= image_path('favicons/favicon.ico') %>">
    <meta name="msapplication-config" content="<%= image_path('favicons/browserconfig.xml') %>">
    <meta name="theme-color" content="#ffffff">


    <link href="https://fonts.googleapis.com/css?family=Nunito:300|Open+Sans:400,400i,600" rel="stylesheet">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>

    <% if Rails.env == 'production' %>
      <%= render('partials/ga') %>
    <% end %>
  </body>
</html>
